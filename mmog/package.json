{
  "name": "@sanjo/mmog",
  "version": "0.0.0",
  "type": "module",
  "description": "MMOG",
  "scripts": {
    "build": "yarn build:proto && esbuild packages/aws/move/index.ts packages/aws/initializeWorld/index.ts packages/aws/move/index.ts packages/aws/onConnect/index.ts packages/aws/onDisconnect/index.ts packages/aws/requestObjects/index.ts packages/aws/growPlants/index.ts packages/aws/authorizer/index.ts --bundle --platform=node --target=node18 --outdir=build/ --sourcemap --minify --external:@aws-sdk/client-apigatewaymanagementapi,@aws-sdk/client-dynamodb,@aws-sdk/lib-dynamodb",
    "build:proto": "protoc --ts_out=. packages/shared/proto/*.proto",
    "build:test": "esbuild packages/shared/test.ts --bundle --platform=node --target=node18 --outfile=build/test.cjs --sourcemap",
    "build:test2": "esbuild packages/shared/test2.ts --bundle --platform=node --target=node18 --outfile=build/test2.cjs --sourcemap",
    "build:test3": "esbuild packages/shared/test3.ts --bundle --platform=node --target=node18 --outfile=build/test3.cjs --sourcemap",
    "build:server": "yarn build:proto && esbuild packages/aws/server/index.ts --bundle --platform=node --target=node20 --outdir=packages/aws/server/build/server/ --sourcemap",
    "deploy": "yarn build && sam deploy",
    "deploy:server": "scp -i \"~/.ssh/MMOG.pem\" packages/aws/server/build/server/* ubuntu@ec2-18-196-153-126.eu-central-1.compute.amazonaws.com:/home/ubuntu/server",
    "deploy:server-run-script": "scp -i \"~/.ssh/MMOG.pem\" packages/aws/server/run.sh ubuntu@ec2-18-196-153-126.eu-central-1.compute.amazonaws.com:/home/ubuntu/server"
  },
  "keywords": [],
  "homepage": "https://github.com/SanjoSolutions/projects/tree/main/mmog",
  "license": "Unlicense",
  "repository": {
    "type": "git",
    "url": "https://github.com/SanjoSolutions/projects.git",
    "directory": "mmog"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "^3.408.0",
    "@aws-sdk/client-dynamodb": "^3.408.0",
    "@aws-sdk/lib-dynamodb": "^3.408.0",
    "aws-jwt-verify": "^4.0.0",
    "google-protobuf": "^3.21.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.119",
    "@types/google-protobuf": "^3.15.6",
    "esbuild": "^0.19.2"
  }
}
